{
  "name": "Frugal Song Finder for Beatport",
  "version": "1.1",
  "description": "Tries to find cheapest song while browsing beatport.",
  "page_action": {
  	"default_name" : "Find cheap songs",
  	"default_icon": "Images/icon.png"
  },
  "content_scripts":[
  {
  	"matches":["http://www.beatport.com/*"],
  	"run_at" : "document_idle",
    "css" : ["bootstrap.min.css"],
  	"js":["jquery.js", "string_score.min.js", "bootstrap-twipsy.js", "bootstrap-popover.js", "content_script.js"]
  }
  ],
  "permissions": [
  	"tabs",
    "http://api.beatport.com/*",
    "http://www.beatport.com/*",
    "http://*/*"
  ]
}